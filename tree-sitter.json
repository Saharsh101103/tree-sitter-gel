{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "parser-directories": ["tree-sitter-gel"],
  "grammars": [
    {
      "name": "gel",
      "camelcase": "gel",
      "title": "Gel",
      "scope": "source.gel",
      "file-types": [
        "gel"
      ],
      "injection-regex": "^gel$",
      "class-name": "TreeSitterGel"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter for el/edgeql",
    "authors": [
      {
        "name": "Saharsh",
        "email": "dev.saharsh10@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-gel"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
