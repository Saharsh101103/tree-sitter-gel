module advocacy {
  alias Object := default::Object;
  alias Ticket := default::Ticket;
  alias nestr := default::nestr;
  alias nat16 := default::nat16;
  alias nat32 := default::nat32;
  alias posint16 := default::posint16;
  alias posint32 := default::posint32;

  scalar type SocialMediaPlatform extending enum<Instagram, Facebook, X>;

  scalar type SocialMediaPostNo extending sequence;
  type SocialMediaPost extending Ticket {
    annotation title := "Social Media Post";
    annotation default::prefix := "SOCIAL";

    overloaded number: SocialMediaPostNo;
  }
}
