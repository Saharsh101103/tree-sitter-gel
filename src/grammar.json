{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gel",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "create_type"
        },
        {
          "type": "SYMBOL",
          "name": "create_scalar_type"
        },
        {
          "type": "SYMBOL",
          "name": "alter_type"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "multi_line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "semicolon"
        }
      ]
    },
    "create_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "CREATE TYPE"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_type_body_element"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_type_body_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field"
        },
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "SYMBOL",
          "name": "constraint"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "multi_line_comment"
        }
      ]
    },
    "field": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "field_attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "DEFAULT"
                },
                {
                  "type": "SYMBOL",
                  "name": "expression"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "link"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "field_attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "DEFAULT"
                },
                {
                  "type": "SYMBOL",
                  "name": "expression"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "field_attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "multi"
        },
        {
          "type": "STRING",
          "value": "required"
        }
      ]
    },
    "constraint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "constraint"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "constraint_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "constraint_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "multi_line_comment"
        }
      ]
    },
    "create_scalar_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "CREATE SCALAR TYPE"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "EXTENDING"
                },
                {
                  "type": "SYMBOL",
                  "name": "type_name"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "alter_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ALTER TYPE"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "alter_action"
          }
        }
      ]
    },
    "alter_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "ADD"
            },
            {
              "type": "STRING",
              "value": "DROP"
            },
            {
              "type": "STRING",
              "value": "ALTER"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_type_body_element"
        }
      ]
    },
    "type_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                {
                  "type": "STRING",
                  "value": "::"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "str"
            },
            {
              "type": "STRING",
              "value": "int"
            },
            {
              "type": "STRING",
              "value": "float"
            },
            {
              "type": "STRING",
              "value": "bool"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "term"
        },
        {
          "type": "SYMBOL",
          "name": "sum"
        }
      ]
    },
    "sum": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "product"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "+"
                    },
                    {
                      "type": "STRING",
                      "value": "-"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "product"
                }
              ]
            }
          }
        ]
      }
    },
    "product": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "term"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "*"
                    },
                    {
                      "type": "STRING",
                      "value": "/"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "term"
                }
              ]
            }
          }
        ]
      }
    },
    "term": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "number"
          },
          {
            "type": "SYMBOL",
            "name": "string"
          },
          {
            "type": "STRING",
            "value": "true"
          },
          {
            "type": "STRING",
            "value": "false"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "("
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              },
              {
                "type": "STRING",
                "value": ")"
              }
            ]
          }
        ]
      }
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]\\w*"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "multi_line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "PATTERN",
          "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
        },
        {
          "type": "STRING",
          "value": "/"
        }
      ]
    },
    "semicolon": {
      "type": "STRING",
      "value": ";"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}